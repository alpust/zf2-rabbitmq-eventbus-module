{
    "name": "paulchyrilov/amqp-zf2-module",
    "description": "Zend 2 amqp module",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "module",
        "zf2",
        "amqp"
    ],
    "require": {
        "php":                               ">=5.3.3",
        "ext-amqp"  : ">=1.4.0",
        "zendframework/zend-stdlib":         ">=2.2",
        "zendframework/zend-mvc":            ">=2.2",
        "zendframework/zend-servicemanager": ">=2.2"
    },
    "require-dev": {
        "pdezwart/php-amqp" : "dev-master"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
            "AMQPModule\\": "src/"
        }
    }
}
